<Window x:Class="AHIFventory.DeleteOrderConfirmationDialogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AHIFventory"
        mc:Ignorable="d"
        Title="Confirmation" Height="150" Width="300">
    <StackPanel Margin="10">
        <TextBlock Text="Do you want to revert changes?" Margin="0,0,0,20" HorizontalAlignment="Center"/>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Button Name="YesButton" Content="Yes" Width="75" Margin="10,0"/>
            <Button Name="NoButton" Content="No" Width="75" Margin="10,0"/>
        </StackPanel>
    </StackPanel>
</Window>
